; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "InputManager",
"language": "GDScript",
"path": "res://Minigames/Base/InputManager.gd"
}, {
"base": "TextureButton",
"class": "LevelChoice",
"language": "GDScript",
"path": "res://LevelPicker/LevelChoice.gd"
}, {
"base": "Control",
"class": "LevelPicker",
"language": "GDScript",
"path": "res://LevelPicker/LevelPicker.gd"
}, {
"base": "Node2D",
"class": "MinigameManager",
"language": "GDScript",
"path": "res://Minigames/Base/MinigameManager.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://Minigames/Base/Player.gd"
}, {
"base": "Node2D",
"class": "PlayerController",
"language": "GDScript",
"path": "res://Minigames/Base/PlayerController.gd"
}, {
"base": "HBoxContainer",
"class": "WinText",
"language": "GDScript",
"path": "res://Minigames/Base/WinText.gd"
} ]
_global_script_class_icons={
"InputManager": "",
"LevelChoice": "",
"LevelPicker": "",
"MinigameManager": "",
"Player": "",
"PlayerController": "",
"WinText": ""
}

[application]

config/name="GodotMinigames"
run/main_scene="res://Minigames/Spaceship/SpaceshipMinigame.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://Minigames/Base/Global.gd"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/handheld/emulate_mouse_from_touch=false
"window/Use Vsync"=true

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Hazards"
2d_physics/layer_3="Boosts"

[rendering]

quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=0
quality/2d/use_pixel_snap=true
vram_compression/import_etc=true
environment/default_environment="res://default_env.tres"
